<?xml version="1.0" encoding="UTF-8"?>
<ehcache>
	<diskStore path="C:/cache/" />
	<defaultCache
		maxElementsInMemory="10000" 
		eternal="false"
		timeToIdleSeconds="120" 
		timeToLiveSeconds="120" 
		overflowToDisk="true"
		diskPersistent="false"
		diskExpiryThreadIntervalSeconds="60" />
		
